{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "-webkit-filter"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "axe/text-alternatives": [
      "default",
      {
        "input-image-alt": "off",
        "image-alt": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  }
}